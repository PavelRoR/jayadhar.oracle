#main
	padding 50px 0
	background-color: #CAD1CD
	background-image url(../img/backs/course-main-bg.jpg)
	background-repeat no-repeat
	background-position: center left
	background-size: cover
	text-align: center
.pre
	font-size: 18px
.main-title
	font-size: 54px
	font-family: J
	line-height: 38px
	margin 20px 0 40px
	span
		font-size: 22px
		display block
		margin-top: 10px
	small
		font-family: OSSB
		font-size: 18px
		display block
.main-info
	margin-bottom: 15px
	font-size: 20px
	span
		font-size: 24px
		display: block
		font-family: OSB
		color #0a1938
	&:last-of-type
		margin-bottom: 40px