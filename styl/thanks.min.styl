.checking {
	display: table;
	width 100%
	max-width 502px
	margin: 5px auto 0
	text-align: left
}

.checking .check-label {
	line-height: 27px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 25px;
	text-align: left;
	font-size: 14px
	color #0a1938
}

.checking .check-label a
	text-decoration: underline
	display: inline
	color #0a1938
	&:focus
		text-decoration: underline
		color #0a1938
	&:hover
		text-decoration: none
		color #0a1938

.checking .check-label:before {
	content: '';
	background-color: red
	position: absolute;
	left: 0;
	top: 3px;
	width: 20px;
	height: 20px;
	border 1px dashed #0a1938
	border-radius: 0
}

.checking .check-label:after {
	content: '';
	position: absolute;
	left: 8px;
	top: -10px;
	width: 6px;
	height: 11px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #0a1938
	border-right: 2px solid #0a1938
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;

}

.check {
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
}

.check:checked + .check-label:before {
	background-color: transparent

}

.check:checked + .check-label:after {
	opacity: 1;
	top: 7px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}
