*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: OS
	background-color #fff
	color #2a251d
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	margin 0 auto
	border 4px outset rgb(216, 190, 123);
	background-color: rgb(216, 190, 123);
	width 100%
	max-width: 365px;
	height: 46px;
	line-height: 36px;
	text-decoration: none
	text-align: center
	transition: all .5s ease
	&:focus
		color initial 
		text-decoration: none
	&:hover
		color initial 
		text-decoration: none
		background-color: rgb(216, 190, 123) - 15%
		transform: translateY(-4px)
	&-form
		color #fff
		font-family: J
		font-size: 24px
		background-color: #568DCB
		border: none
		max-width 223px
		height 36px
		line-height 36px
		&:focus
			color #fff
		&:hover
			color #fff
			background-color: #568DCB - 15%
.general-title
	text-align: center
	font-size: 36px;
	font-family: J
	text-decoration: underline;
	line-height: 0.833;
	margin-bottom: 50px
	span
		text-decoration: none
		color #d1a430
		display: inline-block
.input
	display block
	background-color: rgb(255, 255, 255);
	width 100%
	max-width: 223px;
	padding 0 15px
	height: 36px;
	border-radius 0
	border none
	line-height: 36px
@-webkit-keyframes autofill
	to
		color: initial
		background-color: #fff
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.alert-message {
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	line-height: 16px
	color: #f00
	text-align: center
}
.checking {
	display: table;
	width 100%
	max-width 477px
	margin: 5px auto 0
	text-align: left
}

.checking .check-label {
	line-height: 27px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 25px;
	text-align: left;
	font-size: 14px
	color #0a1938
}

.checking .check-label a
	text-decoration: underline
	display: inline
	color #0a1938
	&:focus
		text-decoration: underline
		color #0a1938
	&:hover
		text-decoration: none
		color #0a1938

.checking .check-label:before {
	content: '';
	background-color: red
	position: absolute;
	left: 0;
	top: 3px;
	width: 20px;
	height: 20px;
	border 1px dashed #0a1938
	border-radius: 0
}

.checking .check-label:after {
	content: '';
	position: absolute;
	left: 8px;
	top: -10px;
	width: 6px;
	height: 11px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #0a1938
	border-right: 2px solid #0a1938
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;

}

.check {
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
}

.check:checked + .check-label:before {
	background-color: transparent

}

.check:checked + .check-label:after {
	opacity: 1;
	top: 7px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}

